abstract class BooleanBinOperator{
    abstract boolen op(boolean x, boolean y);
    @Override
    public abstract String toString();}

classAddition extends BooleanBinOperator{
    boolean op(boolean x, boolean y){
	return x || y;
    }
    @Override
	public String toString(){
	return "+";
    }
}

class Multplication extends BooleanBinOperator{
    bolen op(boolean x, boolean y){
	return && y;
    }
    @Override
    public String toString(){
	return "*";
    }
}

